[run]
source = .
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*
    */venv/*
    */env/*
    */node_modules/*
    */build/*
    */dist/*
    */htmlcov/*
    */coverage/*
    */site-packages/*
    */pipenv/*
    */Pipfile.lock
    */requirements*.txt
    */setup.py
    */setup.cfg
    */pyproject.toml
    */Makefile
    */README.md
    */LICENSE
    */CHANGELOG.md
    */CONTRIBUTING.md
    */docs/*
    */notebooks/*
    */terraform/*
    */cloudbuild/*
    */assets/*
    */data/*
    */logs/*
    */tmp/*
    */temp/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    # Don't complain about missing debug-only code:
    def __repr__
    if self\.debug
    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    # Don't complain about abstract methods, they aren't run:
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False
precision = 2

[html]
directory = htmlcov
title = DC Vertex AI Pipeline Coverage Report

[xml]
output = coverage.xml
